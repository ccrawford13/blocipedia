<div class="container user">
  <div class="user-text">
  <% if current_user == @user %>
    <h1>My Wikis</h1>
  <% else %>
    <h1> <%= @user.name %> </h1> 
  <% end %>
  </div>
  <div class="wiki-box">
    <%= render @wikis %>
  </div> 
  <div class="row">
    <div class="col-md-4 col-md-offset-6">
      <div class="new-btn">
        <% if current_user == @user %>
          <%= link_to "New Wiki", new_wiki_path(@user, @wiki), class: 'btn btn-success' %>
        <% end %>
      </div>
    </div>
  </div>
</div>


